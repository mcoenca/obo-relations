		(and
			(downstream_neural_circuit_path ...s ?x ?y)
			("neuron ; CL_0000540" ?z)
			(synapsed_by ?y ?z))))
			
(forall (?x ?y) 
	(iff 
		(downstream_in_neural_circuit_with ?x ?y)
		(exists (...s)
			(downstream_neural_circuit_path  ?x ...s ?y)))) 
			"^^xsd:string"
