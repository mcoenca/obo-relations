holds_over_chain: BFO:0000051 BFO:0000050 ! has part part of
holds_over_chain: RO:0002131 BFO:0000050 ! overlaps part of
holds_over_chain: BFO:0000051 RO:0002131 ! has part overlaps
expand_expression_to: "http://purl.obolibrary.org/obo/BFO_0000051 some (http://purl.obolibrary.org/obo/BFO_0000050 some ?Y)" []

[Typedef]
