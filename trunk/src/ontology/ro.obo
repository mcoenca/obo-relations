is_a: RO:0002131 ! overlaps
holds_over_chain: RO:0002130 BFO:0000050 ! has_synaptic_terminal_in part of
expand_expression_to: "RO_0002131 some (GO_0045202 that BFO_0000050 some Y?)" []

[Typedef]
id: RO:0002131
