     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="&obo;ro/bfo-classes-minimal.owl">
        <owl:imports rdf:resource="&obo;ro/annotations.owl"/>
